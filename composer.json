{
    "name": "survos/landing-bundle",
    "description": "Add some tools and resources to a Symfony4 application",
    "type": "symfony-bundle",
    "keywords": ["generator", "skeleton"],
    "require": {
        "knplabs/knp-menu-bundle": "*",
        "knpuniversity/oauth2-client-bundle": "*",
        "twig/twig": "^2.5|^3.0",
        "symfony/webpack-encore-bundle": "^1.6|^2"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "fzaninotto/faker": "^1.7"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Survos\\LandingBundle\\": "src/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Tac Tacelosky",
            "email": "tacman@gmail.com"
        }
    ],
    "extra": {
        "symfony": {
            "allow-contrib": "true",
            "id": "landing-bundle.survos.com"
        }
    }
}
