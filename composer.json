{
    "name": "survos/landing-bundle",
    "description": "Add some tools and resources to a Symfony5 application",
    "type": "symfony-bundle",
    "keywords": ["generator", "skeleton" , "symfony5"],
    "require": {
        "ext-json": "*",
        "ext-zip": "*",
        "knplabs/knp-menu": "^3.0",
        "knplabs/knp-menu-bundle": "^3.0",
        "knpuniversity/oauth2-client-bundle": "^1.0",
        "meyfa/php-svg": "^0.9.1",
        "symfony/mailer": "*",
        "symfony/orm-pack": "*",
        "symfony/string": "^5.0",
        "symfony/webpack-encore-bundle": "^1.6|^2",
        "symfony/yaml": "*",
        "twig/string-extra": "^3.0",
        "twig/twig": "^2.5|^3.0"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "fzaninotto/faker": "^1.7"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Survos\\LandingBundle\\": "src/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Tac Tacelosky",
            "email": "tacman@gmail.com"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.7.x-dev"
        },
        "symfony": {
            "allow-contrib": "true",
            "id": "landing-bundle.survos.com"
        }
    }
}
