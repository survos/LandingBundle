{% extends "base.html.twig" %}

{% block body %}

    <table class="table table-striped table-condensed">
        <thead>
        <tr>
            <th>Bundle</th>
            <th>Minimum Version</th>
        </tr>
        </thead>
        <tbody>
        {% for bundle, version in composerData.require %}
            <tr>
                <td>{{ bundle }}</td>
                <td>{{ version }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
{% endblock %}